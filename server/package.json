{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node build/server.js",
    "dev": " ts-node-dev src/server.ts",
    "test": "jest --watch-all --no-cache ",
    "test:open": "jest --watch-all --no-cache --detectOpenHandles ",
    "clean": "ts-node-dev scripts/clearDB.ts",
    "data": "ts-node-dev scripts/fancyData.ts",
    "stop": "pm2 stop all",
    "refresh": "npm run clean && npm run data",
    "serve": "npm run postinstall && pm2 start build/src/server.js -i max --watch && pm2 monit",
    "logs": "heroku logs --tail --app storzey ",
    "logsv2": "heroku logs --tail --app storzeyv2 ",
    "postinstall": "tsc"
  },
  "author": "tsriramaraju",
  "license": "MIT",
  "dependencies": {
    "@slack/web-api": "^6.7.2",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "googleapis": "^101.0.0",
    "helmet": "^5.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.6",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.5",
    "razorpay": "^2.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3",
    "uuid": "^8.3.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.42",
    "@types/nodemailer": "^6.4.4",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {}
}
